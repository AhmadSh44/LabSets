#include <iostream>
using namespace std;

// Function to swap two numbers
void swap(float &number1, float &number2) {
    float temp = number1;
    number1 = number2;
    number2 = temp;
}

int main() {
    float first, second;

    // Reading two floating point numbers
    cout << "Enter the first number: ";
    cin >> first;
    cout << "Enter the second number: ";
    cin >> second;

    cout << "\nBefore swapping:\n";
    cout << "First number: " << first << "\n";
    cout << "Second number: " << second << "\n";

    // Calling the swap function
    swap(first, second);

    cout << "\nAfter swapping:\n";
    cout << "First number: " << first << "\n";
    cout << "Second number: " << second << "\n";

    return 0;
}

